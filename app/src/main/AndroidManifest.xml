<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <permission
        android:name="edu.cas.PERMISO_APP"
        android:label="permiso_personalizado"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CNTGSYM">
        <activity
            android:name=".contactos.SeleccionContactosActivityPermisos"
            android:exported="false" />
        <activity
            android:name=".contactos.SeleccionContactosActivity"
            android:exported="false" />
        <activity
            android:name=".ActividadB"
            android:exported="true"
            android:permission="edu.cas.PERMISO_APP">
            <intent-filter>
                <action android:name=".Saludo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>